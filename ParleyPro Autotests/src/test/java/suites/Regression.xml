    </test>

    <test name="AT-141: Changes not tracked" preserve-order="true">
        <classes>
            <class name="tests.LoginToTrackChangesTenant"/>
            <class name="tests.regression.at141.CreateContractUploadDocAndChange"/>
            <class name="tests.Logout"/>
        </classes>
    </test>
</suite>